#################################################################
# Persona profiles and recommendation classes
#################################################################

# Link profile to prioritized workloads
gad:prioritizesWorkload a owl:ObjectProperty ;
  rdfs:domain gad:ProfilKebutuhan ;
  rdfs:range  gad:Workload ;
  rdfs:label  "prioritizes workload" .

# Profiles
gad:Kebutuhan_AI_Student a gad:ProfilKebutuhan ;
  rdfs:label "Kebutuhan Mahasiswa AI/ML" ;
  gad:prioritizesWorkload gad:Training_Model_AI ,
                          gad:Mobile_Build .

gad:Kebutuhan_MobileDev a gad:ProfilKebutuhan ;
  rdfs:label "Kebutuhan Mobile Developer" ;
  gad:prioritizesWorkload gad:Mobile_Build .

gad:Kebutuhan_KreatorKonten a gad:ProfilKebutuhan ;
  rdfs:label "Kebutuhan Kreator Konten" ;
  gad:prioritizesWorkload gad:Video_Editing ,
                          gad:Mobile_Photo_Video ,
                          gad:Mirrorless_4k_Cine .

# Personas
gad:Persona_AI_Student a gad:Persona ;
  rdfs:label "Mahasiswa AI/ML" ;
  gad:hasPersonaNeed gad:Kebutuhan_AI_Student .

gad:Persona_MobileDev a gad:Persona ;
  rdfs:label "Mobile Developer" ;
  gad:hasPersonaNeed gad:Kebutuhan_MobileDev .

gad:Persona_KreatorKonten a gad:Persona ;
  rdfs:label "Kreator Konten" ;
  gad:hasPersonaNeed gad:Kebutuhan_KreatorKonten .

# Recommended classes per persona
gad:RecommendedFor_AI_Student a owl:Class ;
  rdfs:label "Recommended for AI/ML Student" ;
  owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
      gad:Gadget
      [ a owl:Restriction ;
        owl:onProperty gad:meetsRequirement ;
        owl:someValuesFrom [ a owl:Class ; owl:oneOf ( gad:Training_Model_AI gad:Mobile_Build ) ]
      ]
    )
  ] .

gad:RecommendedFor_MobileDev a owl:Class ;
  rdfs:label "Recommended for Mobile Developer" ;
  owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
      gad:Gadget
      [ a owl:Restriction ;
        owl:onProperty gad:meetsRequirement ;
        owl:someValuesFrom [ a owl:Class ; owl:oneOf ( gad:Mobile_Build ) ]
      ]
    )
  ] .

gad:RecommendedFor_KreatorKonten a owl:Class ;
  rdfs:label "Recommended for Content Creator" ;
  owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
      gad:Gadget
      [ a owl:Restriction ;
        owl:onProperty gad:meetsRequirement ;
        owl:someValuesFrom [ a owl:Class ; owl:oneOf ( gad:Video_Editing gad:Mobile_Photo_Video gad:Mirrorless_4k_Cine ) ]
      ]
    )
  ] .
