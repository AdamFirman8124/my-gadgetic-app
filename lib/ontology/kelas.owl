@prefix :       <http://example.org/gadgetic#> .
@prefix gad:    <http://example.org/gadgetic#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix swrl:   <http://www.w3.org/2003/11/swrl#> .
@prefix swrlb:  <http://www.w3.org/2003/11/swrlb#> .

#################################################################
# Ontology Header
#################################################################

<http://example.org/gadgetic>
  a owl:Ontology ;
  rdfs:label "GadgeTic Ontology SROIQ(D)" ;
  rdfs:comment "Ontology with 90 devices. Optimized for SROIQ(D) expressivity by adding Disjoint Classes and Nominals." .

#################################################################
# SROIQ Requirement: Disjoint Classes (Negation/Complement - 'C')
#################################################################

# Ini adalah KUNCI agar statusnya berubah dari AL... menjadi S...
# Kita menegaskan bahwa Laptop TIDAK BISA menjadi Smartphone, dst.

[ a owl:AllDisjointClasses ;
  owl:members ( gad:Laptop gad:Smartphone gad:Drone gad:Smartwatch gad:Smartband gad:AudioDevice gad:Mirrorless )
] .

[ a owl:AllDisjointClasses ;
  owl:members ( gad:CPU gad:GPU gad:RAM gad:Storage gad:SoC gad:Battery gad:Display gad:CameraModule gad:HealthSensor )
] .

#################################################################
# SROIQ Requirement: Nominals (Enumerated Classes - 'O')
#################################################################

# Kelas yang didefinisikan dengan enumerasi individu (oneOf)
gad:MarketLeaderBrand a owl:Class ;
    owl:equivalentClass [
        a owl:Class ;
        owl:oneOf ( gad:Apple gad:Samsung )
    ] .

gad:BigThreeCamera a owl:Class ;
    owl:equivalentClass [
        a owl:Class ;
        owl:oneOf ( gad:Sony gad:Canon gad:Nikon )
    ] .

#################################################################
# Core Classes & Restrictions (Qualified Cardinality - 'Q')
#################################################################

gad:Gadget a owl:Class .

gad:Laptop a owl:Class ;
  rdfs:subClassOf gad:Gadget ,
  [ a owl:Restriction ; owl:onProperty gad:hasComponent ; owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ; owl:onClass gad:CPU ] ,
  [ a owl:Restriction ; owl:onProperty gad:hasComponent ; owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ; owl:onClass gad:RAM ] .

gad:Smartphone a owl:Class ;
  rdfs:subClassOf gad:Gadget ,
  [ a owl:Restriction ; owl:onProperty gad:hasComponent ; owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ; owl:onClass gad:SoC ] .

gad:Drone a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Smartwatch a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Smartband a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:AudioDevice a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Mirrorless a owl:Class ; rdfs:subClassOf gad:Gadget .

gad:Component a owl:Class .
gad:CPU a owl:Class ; rdfs:subClassOf gad:Component .
gad:GPU a owl:Class ; rdfs:subClassOf gad:Component .
gad:RAM a owl:Class ; rdfs:subClassOf gad:Component .
gad:Storage a owl:Class ; rdfs:subClassOf gad:Component .
gad:SoC a owl:Class ; rdfs:subClassOf gad:Component .
gad:Battery a owl:Class ; rdfs:subClassOf gad:Component .
gad:Display a owl:Class ; rdfs:subClassOf gad:Component .
gad:CameraModule a owl:Class ; rdfs:subClassOf gad:Component .
gad:HealthSensor a owl:Class ; rdfs:subClassOf gad:Component .

gad:Workload a owl:Class .
gad:Brand a owl:Class .
gad:DeviceCategory a owl:Class .

#################################################################
# Object Properties (S, R, I)
#################################################################

# 1. Complex Roles (R): Asymmetric & Irreflexive
# Syarat SROIQ: Properti yang punya karakteristik ini harus 'Simple' (tidak Transitif)
gad:hasComponent a owl:ObjectProperty, owl:AsymmetricProperty, owl:IrreflexiveProperty ;
    owl:inverseOf gad:isComponentOf ;
    rdfs:domain gad:Gadget ;
    rdfs:range gad:Component .

gad:isComponentOf a owl:ObjectProperty, owl:AsymmetricProperty, owl:IrreflexiveProperty ;
    rdfs:domain gad:Component ;
    rdfs:range gad:Gadget .

# 2. Functional & Disjoint (R)
gad:hasBrand a owl:ObjectProperty, owl:FunctionalProperty ;
    owl:inverseOf gad:isBrandOf ;
    rdfs:domain gad:Gadget ;
    rdfs:range gad:Brand ;
    owl:propertyDisjointWith gad:hasComponent . 

gad:isBrandOf a owl:ObjectProperty, owl:InverseFunctionalProperty ;
    rdfs:domain gad:Brand ;
    rdfs:range gad:Gadget .

# 3. Transitive Properties (S implies Transitive)
gad:parentBrand a owl:ObjectProperty , owl:TransitiveProperty ; 
    owl:inverseOf gad:hasSubBrand ; 
    rdfs:domain gad:Brand ; 
    rdfs:range gad:Brand .

gad:hasSubBrand a owl:ObjectProperty , owl:TransitiveProperty ; 
    rdfs:domain gad:Brand ; 
    rdfs:range gad:Brand .

# 4. Property Chains (R)
gad:brandForCategory a owl:ObjectProperty ; 
    rdfs:domain gad:Brand ; 
    rdfs:range gad:DeviceCategory ;
    owl:propertyChainAxiom ( gad:parentBrand gad:brandForCategory ) .

# 5. Standard Relations
gad:meetsRequirement a owl:ObjectProperty ; owl:inverseOf gad:isRequirementMetBy ; rdfs:domain gad:Gadget ; rdfs:range gad:Workload .
gad:isRequirementMetBy a owl:ObjectProperty ; rdfs:domain gad:Workload ; rdfs:range gad:Gadget .

