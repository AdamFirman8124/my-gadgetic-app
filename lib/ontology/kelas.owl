@prefix :       <http://example.org/gadgetic#> .
@prefix gad:    <http://example.org/gadgetic#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .

#################################################################
# Ontology header
#################################################################

<http://example.org/gadgetic>
  a owl:Ontology ;
  rdfs:label "GadgeTic Ontology" ;
  rdfs:comment "Ontology for gadget recommendations; Indonesia market device coverage; no PC." .

#################################################################
# Core classes
#################################################################

gad:Gadget            a owl:Class .
gad:Laptop            a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Smartphone        a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Drone             a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Smartwatch        a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Smartband         a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:AudioDevice       a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Mirrorless        a owl:Class ; rdfs:subClassOf gad:Gadget .

gad:Component         a owl:Class .
gad:CPU               a owl:Class ; rdfs:subClassOf gad:Component .
gad:GPU               a owl:Class ; rdfs:subClassOf gad:Component .
gad:RAM               a owl:Class ; rdfs:subClassOf gad:Component .
gad:Storage           a owl:Class ; rdfs:subClassOf gad:Component .
gad:SoC               a owl:Class ; rdfs:subClassOf gad:Component .
gad:Battery           a owl:Class ; rdfs:subClassOf gad:Component .
gad:Display           a owl:Class ; rdfs:subClassOf gad:Component .
gad:CameraModule      a owl:Class ; rdfs:subClassOf gad:Component .
gad:HealthSensor      a owl:Class ; rdfs:subClassOf gad:Component .

gad:Persona           a owl:Class .
gad:ProfilKebutuhan   a owl:Class .
gad:Workload          a owl:Class .

# Brand modeling
gad:Brand             a owl:Class .
gad:DeviceCategory    a owl:Class .

#################################################################
# Object properties
#################################################################

gad:hasComponent a owl:ObjectProperty ;
  rdfs:domain gad:Gadget ;
  rdfs:range  gad:Component ;
  rdfs:label "has component" .

gad:isComponentOf a owl:ObjectProperty ;
  owl:inverseOf gad:hasComponent ;
  rdfs:domain gad:Component ;
  rdfs:range  gad:Gadget ;
  rdfs:label "is component of" .

gad:meetsRequirement a owl:ObjectProperty ;
  rdfs:domain gad:Gadget ;
  rdfs:range  gad:Workload ;
  rdfs:label "meets requirement" .

gad:needsComponent a owl:ObjectProperty ;
  rdfs:domain gad:Workload ;
  rdfs:range  gad:Component ;
  rdfs:label "needs component" .

gad:hasPersonaNeed a owl:ObjectProperty ;
  rdfs:domain gad:Persona ;
  rdfs:range  gad:ProfilKebutuhan ;
  rdfs:label "has persona need" .

gad:hasBrand a owl:ObjectProperty ;
  rdfs:domain gad:Gadget ;
  rdfs:range  gad:Brand ;
  rdfs:label "has brand" .

gad:brandForCategory a owl:ObjectProperty ;
  rdfs:domain gad:Brand ;
  rdfs:range  gad:DeviceCategory ;
  rdfs:label "brand for category" .

gad:parentBrand a owl:ObjectProperty ;
  rdfs:domain gad:Brand ;
  rdfs:range  gad:Brand ;
  rdfs:label "parent brand" .

# Inherit categories from parent brand: parentBrand o brandForCategory âŠ‘ brandForCategory
gad:brandForCategory owl:propertyChainAxiom ( gad:parentBrand gad:brandForCategory ) .

#################################################################
# Data properties
#################################################################

gad:ramSize a owl:DatatypeProperty ;
  rdfs:domain gad:RAM ;
  rdfs:range xsd:integer ;
  rdfs:label "RAM size in GB" .

gad:supportsCUDA a owl:DatatypeProperty ;
  rdfs:domain gad:GPU ;
  rdfs:range xsd:boolean ;
  rdfs:label "GPU supports CUDA" .

gad:vramSize a owl:DatatypeProperty ;
  rdfs:domain gad:GPU ;
  rdfs:range xsd:integer ;
  rdfs:label "VRAM size in GB" .

gad:cpuModelName a owl:DatatypeProperty ;
  rdfs:domain gad:CPU ;
  rdfs:range xsd:string ;
  rdfs:label "CPU model name" .

gad:socModelName a owl:DatatypeProperty ;
  rdfs:domain gad:SoC ;
  rdfs:range xsd:string ;
  rdfs:label "SoC model name" .

gad:supportsNPU a owl:DatatypeProperty ;
  rdfs:domain gad:SoC ;
  rdfs:range xsd:boolean ;
  rdfs:label "SoC has NPU" .

gad:cellular5G a owl:DatatypeProperty ;
  rdfs:domain gad:SoC ;
  rdfs:range xsd:boolean ;
  rdfs:label "Cellular 5G" .

gad:batteryCapacitymAh a owl:DatatypeProperty ;
  rdfs:domain gad:Battery ;
  rdfs:range xsd:integer ;
  rdfs:label "Battery capacity mAh" .

gad:batteryCapacityWh a owl:DatatypeProperty ;
  rdfs:domain gad:Battery ;
  rdfs:range xsd:decimal ;
  rdfs:label "Battery capacity Wh" .

gad:storageSizeGB a owl:DatatypeProperty ;
  rdfs:domain gad:Storage ;
  rdfs:range xsd:integer ;
  rdfs:label "Storage size GB" .

gad:displaySizeInch a owl:DatatypeProperty ;
  rdfs:domain gad:Display ;
  rdfs:range xsd:decimal ;
  rdfs:label "Display size inch" .

gad:refreshRateHz a owl:DatatypeProperty ;
  rdfs:domain gad:Display ;
  rdfs:range xsd:integer ;
  rdfs:label "Refresh rate Hz" .

gad:panelType a owl:DatatypeProperty ;
  rdfs:domain gad:Display ;
  rdfs:range xsd:string ;
  rdfs:label "Panel type" .

gad:cameraMegapixel a owl:DatatypeProperty ;
  rdfs:domain gad:CameraModule ;
  rdfs:range xsd:integer ;
  rdfs:label "Camera megapixel" .

gad:hasOIS a owl:DatatypeProperty ;
  rdfs:domain gad:CameraModule ;
  rdfs:range xsd:boolean ;
  rdfs:label "Has OIS" .

gad:ibis a owl:DatatypeProperty ;
  rdfs:domain gad:CameraModule ;
  rdfs:range xsd:boolean ;
  rdfs:label "In-body stabilization" .

gad:sensorFormat a owl:DatatypeProperty ;
  rdfs:domain gad:CameraModule ;
  rdfs:range xsd:string ;
  rdfs:label "Sensor format" .

gad:lensMount a owl:DatatypeProperty ;
  rdfs:domain gad:CameraModule ;
  rdfs:range xsd:string ;
  rdfs:label "Lens mount" .

gad:video4k60 a owl:DatatypeProperty ;
  rdfs:domain gad:CameraModule ;
  rdfs:range xsd:boolean ;
  rdfs:label "4K60 video capability" .

gad:weightGr a owl:DatatypeProperty ;
  rdfs:domain gad:Gadget ;
  rdfs:range xsd:integer ;
  rdfs:label "Weight in grams" .

gad:maxFlightTimeMin a owl:DatatypeProperty ;
  rdfs:domain gad:Drone ;
  rdfs:range xsd:integer ;
  rdfs:label "Max flight time in minutes" .

gad:osName a owl:DatatypeProperty ;
  rdfs:domain gad:Gadget ;
  rdfs:range xsd:string ;
  rdfs:label "Operating system name" .

#################################################################
# Workloads
#################################################################

gad:Training_Model_AI a gad:Workload ;
  rdfs:label "Training Model AI" ;
  gad:needsComponent gad:HighCUDA_GPU, gad:RAM32GB_or_More .

gad:Video_Editing a gad:Workload ;
  rdfs:label "Video Editing" ;
  gad:needsComponent gad:HighVRAM_GPU, gad:RAM32GB_or_More .

gad:Mobile_Build a gad:Workload ;
  rdfs:label "Mobile Build" ;
  gad:needsComponent gad:MidCPU, gad:RAM16GB_or_More .

gad:Aerial_Videography a gad:Workload ;
  rdfs:label "Aerial Videography" ;
  gad:needsComponent gad:DroneCamera_4K60, gad:DroneBattery_LongFlight .

gad:Wrist_Health_Tracking a gad:Workload ;
  rdfs:label "Wrist Health Tracking" ;
  gad:needsComponent gad:HealthSensorSuite, gad:Battery_LongLife .

gad:Mobile_Photo_Video a gad:Workload ;
  rdfs:label "Mobile Photo Video" ;
  gad:needsComponent gad:SoC_With_NPU, gad:Camera_OIS, gad:Display_120Hz .

gad:Gaming_1440p a gad:Workload ;
  rdfs:label "Gaming 1440p" ;
  gad:needsComponent gad:GPU_1440p, gad:RAM16GB_or_More .

gad:Mirrorless_4k_Cine a gad:Workload ;
  rdfs:label "Mirrorless 4K Cine" ;
  gad:needsComponent gad:IBIS_Cam, gad:Cam_4K60 .

