#################################################################
# Logic & Inference
#################################################################

# Definisi: Apa itu "Kamera Profesional"?
gad:ProfessionalCamera a owl:Class ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (
            gad:Mirrorless
            [ a owl:Restriction ; owl:onProperty gad:hasComponent ; owl:someValuesFrom 
                [ a owl:Restriction ; owl:onProperty gad:sensorFormat ; owl:hasValue "Full Frame" ]
            ]
        )
    ] .

# Definisi: Rekomendasi Kreator Konten
gad:RecommendedFor_KreatorKonten a owl:Class ;
  rdfs:label "Recommended for Content Creator" ;
  owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
      gad:Gadget
      [ a owl:Restriction ;
        owl:onProperty gad:hasComponent ;
        owl:someValuesFrom [ a owl:Restriction ; owl:onProperty gad:video4k60 ; owl:hasValue true ]
      ]
    )
  ] .