@prefix :       <http://example.org/gadgetic#> .
@prefix gad:    <http://example.org/gadgetic#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .

#################################################################
# Ontology Header
#################################################################

<http://example.org/gadgetic>
  a owl:Ontology ;
  rdfs:label "GadgeTic Ontology Ultimate" ;
  rdfs:comment "Ontology gadget lengkap gabungan data lama & baru (SROIQ)." .

#################################################################
# Classes (TBox)
#################################################################

gad:Gadget            a owl:Class .
gad:DeviceCategory    a owl:Class .
gad:Brand             a owl:Class .

# Hierarchy
gad:Laptop            a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Smartphone        a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Tablet            a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Drone             a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Wearable          a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:AudioDevice       a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Camera            a owl:Class ; rdfs:subClassOf gad:Gadget .

# Specific Subclasses
gad:Smartwatch        a owl:Class ; rdfs:subClassOf gad:Wearable .
gad:Smartband         a owl:Class ; rdfs:subClassOf gad:Wearable .
gad:Mirrorless        a owl:Class ; rdfs:subClassOf gad:Camera .
gad:Headphones        a owl:Class ; rdfs:subClassOf gad:AudioDevice .
gad:TWS               a owl:Class ; rdfs:subClassOf gad:AudioDevice .

# Components
gad:Component         a owl:Class .
gad:CPU               a owl:Class ; rdfs:subClassOf gad:Component .
gad:GPU               a owl:Class ; rdfs:subClassOf gad:Component .
gad:RAM               a owl:Class ; rdfs:subClassOf gad:Component .
gad:Storage           a owl:Class ; rdfs:subClassOf gad:Component .
gad:SoC               a owl:Class ; rdfs:subClassOf gad:Component .
gad:Battery           a owl:Class ; rdfs:subClassOf gad:Component .
gad:Display           a owl:Class ; rdfs:subClassOf gad:Component .
gad:CameraModule      a owl:Class ; rdfs:subClassOf gad:Component .
gad:HealthSensor      a owl:Class ; rdfs:subClassOf gad:Component .

# Persona & Logic
gad:Persona           a owl:Class .
gad:ProfilKebutuhan   a owl:Class .
gad:Workload          a owl:Class .

#################################################################
# Object Properties (R & I)
#################################################################

gad:hasComponent a owl:ObjectProperty ;
  rdfs:domain gad:Gadget ;
  rdfs:range  gad:Component ;
  owl:inverseOf gad:isComponentOf ;
  rdfs:label "has component" .

gad:isComponentOf a owl:ObjectProperty ;
  rdfs:domain gad:Component ;
  rdfs:range  gad:Gadget .

gad:hasBrand a owl:ObjectProperty ;
  rdfs:domain gad:Gadget ;
  rdfs:range  gad:Brand .

gad:meetsRequirement a owl:ObjectProperty ;
  rdfs:domain gad:Gadget ;
  rdfs:range  gad:Workload .

gad:needsComponent a owl:ObjectProperty ;
  rdfs:domain gad:Workload ;
  rdfs:range  gad:Component .

gad:hasPersonaNeed a owl:ObjectProperty ;
  rdfs:domain gad:Persona ;
  rdfs:range  gad:ProfilKebutuhan .

gad:prioritizesWorkload a owl:ObjectProperty ;
  rdfs:domain gad:ProfilKebutuhan ;
  rdfs:range  gad:Workload .

gad:brandForCategory a owl:ObjectProperty ;
  rdfs:domain gad:Brand ;
  rdfs:range  gad:DeviceCategory .

gad:parentBrand a owl:ObjectProperty ;
  rdfs:domain gad:Brand ;
  rdfs:range  gad:Brand .

# Property Chain (SROIQ Feature: R)
gad:brandForCategory owl:propertyChainAxiom ( gad:parentBrand gad:brandForCategory ) .

#################################################################
# Data Properties (D)
#################################################################

# --- GENERAL ---
gad:priceIDR a owl:DatatypeProperty ; rdfs:domain gad:Gadget ; rdfs:range xsd:integer .
gad:releaseYear a owl:DatatypeProperty ; rdfs:domain gad:Gadget ; rdfs:range xsd:integer .
gad:reviewScore a owl:DatatypeProperty ; rdfs:domain gad:Gadget ; rdfs:range xsd:decimal .
gad:osName a owl:DatatypeProperty ; rdfs:domain gad:Gadget ; rdfs:range xsd:string .
gad:weightGr a owl:DatatypeProperty ; rdfs:domain gad:Gadget ; rdfs:range xsd:integer .

# --- COMPUTING ---
gad:ramSize a owl:DatatypeProperty ; rdfs:domain gad:Component ; rdfs:range xsd:integer .
gad:storageSizeGB a owl:DatatypeProperty ; rdfs:domain gad:Component ; rdfs:range xsd:integer .
gad:cpuModelName a owl:DatatypeProperty ; rdfs:domain gad:Component ; rdfs:range xsd:string .
gad:gpuModelName a owl:DatatypeProperty ; rdfs:domain gad:Component ; rdfs:range xsd:string .
gad:supportsCUDA a owl:DatatypeProperty ; rdfs:domain gad:Component ; rdfs:range xsd:boolean .

# --- DISPLAY ---
gad:displaySizeInch a owl:DatatypeProperty ; rdfs:domain gad:Display ; rdfs:range xsd:decimal .
gad:refreshRateHz a owl:DatatypeProperty ; rdfs:domain gad:Display ; rdfs:range xsd:integer .
gad:panelType a owl:DatatypeProperty ; rdfs:domain gad:Display ; rdfs:range xsd:string .

# --- CAMERA / MIRRORLESS ---
gad:cameraMegapixel a owl:DatatypeProperty ; rdfs:domain gad:Component ; rdfs:range xsd:integer .
gad:sensorFormat a owl:DatatypeProperty ; rdfs:domain gad:Component ; rdfs:range xsd:string .
gad:ibis a owl:DatatypeProperty ; rdfs:domain gad:Component ; rdfs:range xsd:boolean .
gad:video4k60 a owl:DatatypeProperty ; rdfs:domain gad:Component ; rdfs:range xsd:boolean .
gad:lensMount a owl:DatatypeProperty ; rdfs:domain gad:Component ; rdfs:range xsd:string .

# --- DRONE ---
gad:maxFlightTimeMin a owl:DatatypeProperty ; rdfs:domain gad:Drone ; rdfs:range xsd:integer .
gad:transmissionRangeKm a owl:DatatypeProperty ; rdfs:domain gad:Drone ; rdfs:range xsd:decimal .

# --- BATTERY ---
gad:batteryCapacitymAh a owl:DatatypeProperty ; rdfs:domain gad:Component ; rdfs:range xsd:integer .
gad:batteryCapacityWh a owl:DatatypeProperty ; rdfs:domain gad:Component ; rdfs:range xsd:decimal .


