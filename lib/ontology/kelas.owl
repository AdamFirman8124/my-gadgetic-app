@prefix :       <http://example.org/gadgetic#> .
@prefix gad:    <http://example.org/gadgetic#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix swrl:   <http://www.w3.org/2003/11/swrl#> .
@prefix swrlb:  <http://www.w3.org/2003/11/swrlb#> .

#################################################################
# Ontology header
#################################################################

<http://example.org/gadgetic>
  a owl:Ontology ;
  rdfs:label "GadgeTic Ontology Ultimate Specs" ;
  rdfs:comment "Ontology with 40 real-world gadgets, verified specs (Weight, Battery Wh/mAh, RAM Type), and SWRL rules." .

#################################################################
# Core classes
#################################################################

gad:Gadget            a owl:Class .

gad:Laptop            a owl:Class ; 
                      rdfs:subClassOf gad:Gadget ,
                        [ a owl:Restriction ; owl:onProperty gad:hasComponent ; owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ; owl:onClass gad:CPU ] ,
                        [ a owl:Restriction ; owl:onProperty gad:hasComponent ; owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ; owl:onClass gad:RAM ] .

gad:Smartphone        a owl:Class ; 
                      rdfs:subClassOf gad:Gadget ,
                        [ a owl:Restriction ; owl:onProperty gad:hasComponent ; owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ; owl:onClass gad:SoC ] .

gad:Drone             a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Smartwatch        a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:AudioDevice       a owl:Class ; rdfs:subClassOf gad:Gadget .
gad:Mirrorless        a owl:Class ; rdfs:subClassOf gad:Gadget .

gad:Component         a owl:Class .
gad:CPU               a owl:Class ; rdfs:subClassOf gad:Component .
gad:GPU               a owl:Class ; rdfs:subClassOf gad:Component .
gad:RAM               a owl:Class ; rdfs:subClassOf gad:Component .
gad:Storage           a owl:Class ; rdfs:subClassOf gad:Component .
gad:SoC               a owl:Class ; rdfs:subClassOf gad:Component .
gad:Battery           a owl:Class ; rdfs:subClassOf gad:Component .
gad:Display           a owl:Class ; rdfs:subClassOf gad:Component .
gad:CameraModule      a owl:Class ; rdfs:subClassOf gad:Component .
gad:HealthSensor      a owl:Class ; rdfs:subClassOf gad:Component .

gad:Persona           a owl:Class .
gad:ProfilKebutuhan   a owl:Class .
gad:Workload          a owl:Class .
gad:Brand             a owl:Class .
gad:DeviceCategory    a owl:Class .

#################################################################
# Object properties
#################################################################

gad:hasComponent  a owl:ObjectProperty ; owl:inverseOf gad:isComponentOf ; rdfs:domain gad:Gadget ; rdfs:range gad:Component .
gad:isComponentOf a owl:ObjectProperty ; rdfs:domain gad:Component ; rdfs:range gad:Gadget .

gad:meetsRequirement a owl:ObjectProperty ; owl:inverseOf gad:isRequirementMetBy ; rdfs:domain gad:Gadget ; rdfs:range gad:Workload .
gad:isRequirementMetBy a owl:ObjectProperty ; rdfs:domain gad:Workload ; rdfs:range gad:Gadget .

gad:hasBrand a owl:ObjectProperty , owl:ReflexiveProperty ; owl:inverseOf gad:isBrandOf ; rdfs:domain gad:Gadget ; rdfs:range gad:Brand .
gad:isBrandOf a owl:ObjectProperty ; rdfs:domain gad:Brand ; rdfs:range gad:Gadget .

gad:parentBrand a owl:ObjectProperty , owl:TransitiveProperty ; rdfs:domain gad:Brand ; rdfs:range gad:Brand .
gad:brandForCategory a owl:ObjectProperty ; owl:propertyChainAxiom ( gad:parentBrand gad:brandForCategory ) .

#################################################################
# Data properties - ULTRA DETAILED SPECS
#################################################################

# General Info
gad:priceIDR a owl:DatatypeProperty ; rdfs:range xsd:integer ; rdfs:label "Harga dalam Rupiah" .
gad:releaseYear a owl:DatatypeProperty ; rdfs:range xsd:integer .
gad:osName a owl:DatatypeProperty ; rdfs:range xsd:string .
gad:weightGr a owl:DatatypeProperty ; rdfs:range xsd:integer ; rdfs:label "Berat dalam Gram" .

# Appearance & Features
gad:availableColor a owl:DatatypeProperty ; rdfs:domain gad:Gadget ; rdfs:range xsd:string .
gad:keyFeature a owl:DatatypeProperty ; rdfs:domain gad:Gadget ; rdfs:range xsd:string .
gad:bodyMaterial a owl:DatatypeProperty ; rdfs:domain gad:Gadget ; rdfs:range xsd:string .

# Technical Specs
gad:ramSize a owl:DatatypeProperty ; rdfs:domain gad:RAM ; rdfs:range xsd:integer .
gad:ramTechnology a owl:DatatypeProperty ; rdfs:domain gad:RAM ; rdfs:range xsd:string . # e.g. LPDDR5X
gad:storageSizeGB a owl:DatatypeProperty ; rdfs:domain gad:Storage ; rdfs:range xsd:integer .
gad:storageTechnology a owl:DatatypeProperty ; rdfs:domain gad:Storage ; rdfs:range xsd:string . # e.g. UFS 4.0
gad:vramSize a owl:DatatypeProperty ; rdfs:domain gad:GPU ; rdfs:range xsd:integer .
gad:supportsCUDA a owl:DatatypeProperty ; rdfs:range xsd:boolean .
gad:supportsNPU a owl:DatatypeProperty ; rdfs:range xsd:boolean .

# Display & Camera
gad:displayPanel a owl:DatatypeProperty ; rdfs:domain gad:Display ; rdfs:range xsd:string .
gad:refreshRateHz a owl:DatatypeProperty ; rdfs:domain gad:Display ; rdfs:range xsd:integer .
gad:cameraMegapixel a owl:DatatypeProperty ; rdfs:range xsd:integer .
gad:video4k60 a owl:DatatypeProperty ; rdfs:range xsd:boolean .
gad:ibis a owl:DatatypeProperty ; rdfs:range xsd:boolean .

# Battery & Power
gad:batteryCapacitymAh a owl:DatatypeProperty ; rdfs:domain gad:Battery ; rdfs:range xsd:integer .
gad:batteryCapacityWh a owl:DatatypeProperty ; rdfs:domain gad:Battery ; rdfs:range xsd:decimal .
gad:maxFlightTimeMin a owl:DatatypeProperty ; rdfs:range xsd:integer .

