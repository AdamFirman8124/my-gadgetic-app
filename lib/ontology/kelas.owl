@prefix : <http://example.org/gadgetic#> .
@prefix gad: <http://example.org/gadgetic#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@base <http://example.org/gadgetic#> .

<http://example.org/gadgetic> rdf:type owl:Ontology ;
                               rdfs:comment "Ontology with 90 devices. Optimized for SROIQ(D) expressivity by adding Disjoint Classes and Nominals." ;
                               rdfs:label "GadgeTic Ontology SROIQ(D)" .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://example.org/gadgetic#brandForCategory
gad:brandForCategory rdf:type owl:ObjectProperty ;
                     rdfs:domain gad:Brand ;
                     rdfs:range gad:DeviceCategory ;
                     owl:propertyChainAxiom ( gad:parentBrand
                                              gad:brandForCategory
                                            ) .


###  http://example.org/gadgetic#hasBrand
gad:hasBrand rdf:type owl:ObjectProperty ;
             owl:inverseOf gad:isBrandOf ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain gad:Gadget ;
             rdfs:range gad:Brand ;
             owl:propertyDisjointWith gad:hasComponent .


###  http://example.org/gadgetic#hasComponent
gad:hasComponent rdf:type owl:ObjectProperty ;
                 owl:inverseOf gad:isComponentOf ;
                 rdfs:domain gad:Gadget ;
                 rdfs:range gad:Component .


###  http://example.org/gadgetic#hasSubBrand
gad:hasSubBrand rdf:type owl:ObjectProperty ;
                owl:inverseOf gad:parentBrand ;
                rdf:type owl:TransitiveProperty ;
                rdfs:domain gad:Brand ;
                rdfs:range gad:Brand .


###  http://example.org/gadgetic#isBrandOf
gad:isBrandOf rdf:type owl:ObjectProperty ,
                       owl:InverseFunctionalProperty ;
              rdfs:domain gad:Brand ;
              rdfs:range gad:Gadget .


###  http://example.org/gadgetic#isComponentOf
gad:isComponentOf rdf:type owl:ObjectProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain gad:Component ;
                  rdfs:range gad:Gadget .


###  http://example.org/gadgetic#isRequirementMetBy
gad:isRequirementMetBy rdf:type owl:ObjectProperty ;
                       owl:inverseOf gad:meetsRequirement ;
                       rdfs:domain gad:Workload ;
                       rdfs:range gad:Gadget .


###  http://example.org/gadgetic#meetsRequirement
gad:meetsRequirement rdf:type owl:ObjectProperty ;
                     rdfs:domain gad:Gadget ;
                     rdfs:range gad:Workload .


###  http://example.org/gadgetic#parentBrand
gad:parentBrand rdf:type owl:ObjectProperty ,
                         owl:TransitiveProperty ;
                rdfs:domain gad:Brand ;
                rdfs:range gad:Brand .


