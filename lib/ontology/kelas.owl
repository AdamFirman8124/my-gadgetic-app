:Rule_Laptop_AI a swrl:Imp ;
  swrl:body (
    [ a swrl:ClassAtom ; swrl:classPredicate gad:Laptop ; swrl:argument1 :l ]
    [ a swrl:IndividualPropertyAtom ; swrl:propertyPredicate gad:hasComponent ; swrl:argument1 :l ; swrl:argument2 :g ]
    [ a swrl:ClassAtom ; swrl:classPredicate gad:GPU ; swrl:argument1 :g ]
    [ a swrl:DatatypePropertyAtom ; swrl:propertyPredicate gad:supportsCUDA ; swrl:argument1 :g ; swrl:argument2 "true"^^xsd:boolean ]
    [ a swrl:DatatypePropertyAtom ; swrl:propertyPredicate gad:vramSize ; swrl:argument1 :g ; swrl:argument2 :v ]
    [ a swrl:BuiltinAtom ; swrl:builtin swrlb:greaterThanOrEqual ; swrl:arguments ( :v 8 ) ]
  ) ;
  swrl:head (
    [ a swrl:IndividualPropertyAtom ; swrl:propertyPredicate gad:meetsRequirement ; swrl:argument1 :l ; swrl:argument2 gad:Training_Model_AI ]
  ) .

