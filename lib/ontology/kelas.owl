        rdfs:subClassOf gad:Component .


###  http://example.org/gadgetic#RecommendedFor_AI_MachineLearning
gad:RecommendedFor_AI_MachineLearning rdf:type owl:Class ;
                                      owl:equivalentClass [ owl:intersectionOf ( gad:Gadget
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty gad:meetsRequirement ;
                                                                                   owl:hasValue gad:Advanced_AI_Training
                                                                                 ]
                                                                               ) ;
                                                            rdf:type owl:Class
                                                          ] ;
                                      rdfs:subClassOf gad:Gadget .


###  http://example.org/gadgetic#RecommendedFor_AI_Student
gad:RecommendedFor_AI_Student rdf:type owl:Class ;
                              owl:equivalentClass [ owl:intersectionOf ( gad:Gadget
                                                                         [ rdf:type owl:Restriction ;
                                                                           owl:onProperty gad:meetsRequirement ;
                                                                           owl:hasValue gad:Training_Model_AI
                                                                         ]
                                                                       ) ;
                                                    rdf:type owl:Class
                                                  ] ;
                              rdfs:subClassOf gad:Gadget .


###  http://example.org/gadgetic#RecommendedFor_KreatorKonten
gad:RecommendedFor_KreatorKonten rdf:type owl:Class ;
                                 owl:equivalentClass [ owl:intersectionOf ( gad:Gadget
                                                                            [ rdf:type owl:Class ;
                                                                              owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                              owl:onProperty gad:meetsRequirement ;
                                                                                              owl:hasValue gad:Aerial_Videography
                                                                                            ]
                                                                                            [ rdf:type owl:Restriction ;
                                                                                              owl:onProperty gad:meetsRequirement ;
                                                                                              owl:hasValue gad:Video_Editing
                                                                                            ]
                                                                                          )
                                                                            ]
                                                                          ) ;
                                                       rdf:type owl:Class
                                                     ] ;
                                 rdfs:subClassOf gad:Gadget .


###  http://example.org/gadgetic#RecommendedFor_Mobile_Developer
